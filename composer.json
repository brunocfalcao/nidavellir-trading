{
    "name": "brunocfalcao/nidavellir-trading",
    "description": "Nidavellir trading engine",
    "keywords": [
        "brunocfalcao"
    ],
    "homepage": "https://github.com/brunocfalcao/nidavellir-trading",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "binance/binance-connector-php": "^1.6",
        "brunocfalcao/laravel-helpers": "dev-master",
        "nelexa/zip": "^4.0",
        "illuminate/contracts": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Nidavellir\\Trading\\": "src/",
            "Nidavellir\\Trading\\Database\\Factories\\": "database/factories/",
            "Nidavellir\\Trading\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nidavellir\\Trading\\TradingServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
